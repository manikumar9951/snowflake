pipeline {
    agent {label 'slave'}
    stages {
        stage('code flow from dev to uat') {
            steps {
                sh '''
                cd /home/slave/codeflow/snowflake
                git checkout uat
                git fetch
                git pull
                git status
                git merge --no-ff -m "codeflow from dev to uat" origin/dev 
                git push
                '''
            }
        }
    }
}  
